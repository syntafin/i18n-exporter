{
    "name": "syntafin/laravel-i18n-exporter",
    "description": "This package provides a command to easily convert the Laravel language files into JSON to be used by your frontend application.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11|^12",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Syntafin\\LaravelI18nExporter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Syntafin",
            "email": "hello@syntafin.dev"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Syntafin\\LaravelI18nExporter\\i18nExporterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
